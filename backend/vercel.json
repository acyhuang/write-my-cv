{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload-resume",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/api/generate-cv",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
